/* pages/hypermarket/hypermarket.wxss */
.goodList{
  width: 100%;
  height: auto;
  display: flex;
  justify-content: space-evenly;
  flex-wrap: wrap
}
.goodBox{
  width:45%;
  height: 350rpx;
  border: 1px solid rgba(0,0,0,0.3);
  margin: 10rpx 0;
}
.goodBox image{
  width: 100%;
  height: 300rpx;
  display: block
}
.goodBox view{
  text-align:center;
  line-height: 50rpx;
  font-size: 32rpx;
}